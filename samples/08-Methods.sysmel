Record: TestPair withFields: #{
    first: Int32.
    second: Int32
}.

TestPair
    withSelector: #increment addMethod: ($$($(TestPair)self :: TestPair) := {
        self first: self first + 1i32.
        self
    }).

printLine(TestPair(1i32. 2i32) increment).
